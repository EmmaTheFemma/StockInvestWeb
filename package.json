{
  "name": "stockinvestweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:sass": "sass  --no-source-map src/sass:public/css",
    "copy:html": "copyfiles -u 1 ./src/*.html public",
    "copy": "npm-run-all --parallel copy:*",
    "watch:html": "onchange \"src/*.njk\" -- npm run copy:njk",
    "watch:sass": "sass  --no-source-map --watch src/sass:public/css",
    "watch": "npm-run-all --parallel watch:*",
    "serve": "browser-sync start --server public --files public",
    "start": "eleventy --serve",
    "build": "eleventy build:*",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmmaTheFemma/StockInvestWeb.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmmaTheFemma/StockInvestWeb/issues"
  },
  "homepage": "https://github.com/EmmaTheFemma/StockInvestWeb#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.17",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.8"
  },
  "dependencies": {
    "chart.js": "^3.7.1",
    "react-chartjs-2": "^4.0.1",
    "react-hook-use-state": "^1.1.0"
  }
}
